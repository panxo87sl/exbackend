<h2>Lista de productos en tiempo real</h2>

<ul id="product-list">
  {{#each products}}
    <li>
      <strong>{{title}}</strong>
      - Precio: ${{price}}
      - Stock:
      {{stock}}
      - SKU:
      {{id}}
    </li>
  {{/each}}
</ul>

<script src="/socket.io/socket.io.js"></script>
<script src="/main.js"></script>